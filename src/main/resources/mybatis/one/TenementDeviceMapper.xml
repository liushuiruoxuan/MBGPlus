<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.generator.mbgplus.one.mapper.TenementDeviceMapper">
    <!-- 开启二级缓存 -->
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.generator.mbgplus.one.model.entity.TenementDevice">
                <id column="id" property="id"/>
                <result column="tenement" property="tenement"/>
                <result column="floor" property="floor"/>
                <result column="region" property="region"/>
                <result column="device_id" property="deviceId"/>
                <result column="device_type" property="deviceType"/>
                <result column="device_address" property="deviceAddress"/>
                <result column="cpn_name" property="cpnName"/>
                <result column="remark" property="remark"/>
                <result column="device_positions" property="devicePositions"/>
                <result column="x" property="x"/>
                <result column="y" property="y"/>
                <result column="is_del" property="isDel"/>
                <result column="create_time" property="createTime"/>
                <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, tenement, floor, region, device_id, device_type, device_address, cpn_name, remark, device_positions, x, y, is_del, create_time, update_time
    </sql>
    <select id="getTenementDeviceList" parameterType="com.generator.mbgplus.one.model.entity.TenementDevice" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from t_tenement_device
        <where>
                <if test="id != null and id != ''">
                   and id = #{id}
                </if>
                <if test="tenement != null and tenement != ''">
                   and tenement = #{tenement}
                </if>
                <if test="floor != null and floor != ''">
                   and floor = #{floor}
                </if>
                <if test="region != null and region != ''">
                   and region = #{region}
                </if>
                <if test="deviceId != null and deviceId != ''">
                   and device_id = #{deviceId}
                </if>
                <if test="deviceType != null and deviceType != ''">
                   and device_type = #{deviceType}
                </if>
                <if test="deviceAddress != null and deviceAddress != ''">
                   and device_address = #{deviceAddress}
                </if>
                <if test="cpnName != null and cpnName != ''">
                   and cpn_name = #{cpnName}
                </if>
                <if test="remark != null and remark != ''">
                   and remark = #{remark}
                </if>
                <if test="devicePositions != null and devicePositions != ''">
                   and device_positions = #{devicePositions}
                </if>
                <if test="x != null and x != ''">
                   and x = #{x}
                </if>
                <if test="y != null and y != ''">
                   and y = #{y}
                </if>
                <if test="isDel != null and isDel != ''">
                   and is_del = #{isDel}
                </if>
                <if test="createTime != null and createTime != ''">
                   and create_time = #{createTime}
                </if>
                <if test="updateTime != null and updateTime != ''">
                   and update_time = #{updateTime}
                </if>
        </where>
    </select>
</mapper>
